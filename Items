from scrapy.item import Item, Field

class BballItem(Item):
    NAME = Field()
    POS = Field()
    TEAM = Field()
    DATE = Field()
    OPP = Field()
    WL = Field()
    SCORE = Field()
    MP = Field()
    FGM = Field()
    FGA = Field()
    #FGP = Field()
    iiiFG = Field()
    iiiFGA = Field()
    #iiiFGP = Field()
    FT = Field()
    FTA = Field()
    #FTP = Field()
    PTS = Field()
    OREB = Field()
    DREB = Field()
    REB = Field()
    AST = Field()
    TO = Field()
    STL = Field()
    BLK = Field()
    FOULS = Field()
    LINK = Field()
    pass
